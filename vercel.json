{
  "version": 2,
  "builds": [
    {
      "src": "server/server.cjs",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/server.cjs"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "env": {
    "MONGODB_URI": "",
    "JWT_SECRET": "superSecretKey",
    "NODE_ENV": "production",
    "BREVO_API_KEY": "",
    "CONTACT_TO_EMAIL": "",
    "CONTACT_FROM_EMAIL": ""
  }
}