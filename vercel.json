{
  "version": 2,
  "builds": [
    {
      "src": "server/server.cjs",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/server.cjs"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/dist/assets/$1"
    },
    {
      "src": "/boat-icon.svg",
      "dest": "/dist/boat-icon.svg"
    },
    {
      "src": "/boats.html",
      "dest": "/dist/boats.html"
    },
    {
      "src": "/boats_detail.html", 
      "dest": "/dist/boats_detail.html"
    },
    {
      "src": "/contact.html",
      "dest": "/dist/contact.html"
    },
    {
      "src": "/services.html",
      "dest": "/dist/services.html"
    },
    {
      "src": "/test-navigation.html",
      "dest": "/dist/test-navigation.html"
    },
    {
      "src": "/",
      "dest": "/dist/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ],
  "env": {
    "MONGODB_URI": "",
    "JWT_SECRET": "superSecretKey",
    "NODE_ENV": "production",
    "BREVO_API_KEY": "",
    "CONTACT_TO_EMAIL": "",
    "CONTACT_FROM_EMAIL": ""
  }
}