{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(boats|services|contact|boats_detail|test-navigation|boat-management|experience|boat|reserve|clear-cache|test-index|test-delete-boat)\\.html", "dest": "/$1.html" },
    { "src": "/(.*\\.(?:js|css|svg|png|jpg|jpeg|gif|webp|ico|json|map))", "dest": "/$1" },
    { "src": "/", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  
  "env": {
    "NODE_ENV": "production"
  }
}


